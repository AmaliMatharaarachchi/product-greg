<div class="table-responsive">
    <table class="table table-striped table-hover">
        <thead>
        <h4>Overview</h4>
        </thead>
        <tbody>
        <tr>
            <td>Service Name</td>
            <td>
                {{name}}
            </td>
        </tr>
        <tr>
            <td>Service Version</td>
            <td>
                {{attributes.overview_version}}
            </td>
        </tr>
        <tr>
            <td>Service Namespace</td>
            <td>
                {{attributes.overview_namespace}}
            </td>
        </tr>
        </tbody>
    </table>
    <table class="table table-striped table-hover">
        <thead>
        <h4>Lifecycle</h4>
        </thead>
        <tbody>
        <tr>
            <th>Name :</th>
            <td>{{lifecycle}}</td>
        </tr>
        <tr>
            <th>State :</th>
            <td>{{lifecycleState}}</td>
        </tr>
        </tbody>
    </table>
    <table class="table table-striped table-hover">
        <thead>
        <h4>Documentation</h4>
        </thead>
        <tbody>
        <tr>
            <th>Document Type</th>
            <th>URL</th>
            <th>Comment</th>
        </tr>
        <tr>
            <td>{{attributes.docLinks_documentType}}</td>
            <td><a href={{attributes.docLinks_url}}>{{attributes.docLinks_url}}</a></td>
            <td>{{attributes.docLinks_documentComment}}</td>
        </tr>
        <tr>
            <td>{{attributes.docLinks_documentType1}}</td>
            <td><a href={{attributes.docLinks_url1}}>{{attributes.docLinks_url1}}</a></td>
            <td>{{attributes.docLinks_documentComment1}}</td>
        </tr>

        <tr>
            <td>{{attributes.docLinks_documentType2}}</td>
            <td><a href={{attributes.docLinks_url2}}>{{attributes.docLinks_url2}}</a></td>
            <td>{{attributes.docLinks_documentComment2}}</td>
        </tr>
        </tbody>
    </table>
    <div id='wsdl-viewer'>
        <legend><h4>WSDL</h4></legend>
        {{#if wsdlContent}}
            <textarea id="wsdlcontent" name="wsdlcontent">{{../wsdlContent}}</textarea>
        {{else}}
            <p>No WSDL Content To Display</p>
        {{/if}}
    </div>

</div>