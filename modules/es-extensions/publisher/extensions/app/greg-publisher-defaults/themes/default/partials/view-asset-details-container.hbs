{{#with assets}}
        <div class="well asset-well">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-2">
                        <img src='{{url ""}}/extensions/app/greg-publisher-defaults/themes/default/imgs/thumbnail.png' class="" alt="Thumb"/>
                    </div>
                    <div class="col-lg-10">
                        <h1>{{name}}</h1>
                        {{#if lifecycleState}} 
                        <i class="fa fa-circle"></i> {{lifecycleState}}
                        {{/if}}
                        <div class="well-description">{{tables.0.fields.createdtime.value}}</div>
                        <div class="well-description">{{tables.0.fields.description.value}}</div>
                        <div> 
                            {{> asset-version-info ..}}
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="row-fluid asset-details-row">
            <div class="span12 asset-content-box">
                <div class="widget wlightblue">
                    <div class="widget-head">
                        <div class="pull-left">Asset Overview</div>
                        <div class="asset-actions pull-right">
                            <span class="asset-show-dependency">
                                <a class="btn btn-default btn-lg active pull-right" href='{{url ""}}/pages/impact?path={{path}}' target="_blank">
                                Show Dependency</a>
                            </span>
                            <!--Show only there are multiple versions-->
                            {{#if ../assetVersions}}
                            <span>
                                <select class="select2 version-select" id="diff-view-version">
                                    {{#each ../../assetVersions}}
                                        <option data-base_path="{{../../path}}"
                                                value="{{this.path}}">{{this.version}}</option>
                                    {{/each}}
                                </select>
                            </span>
                            <span>
                                <a class="btn btn-default btn-lg active pull-right" id="diff-view-button"
                                   href="{{url ""}}/pages/diff?type=wsdl&path={{../../assetVersions.0.path}},{{../path}}"
                                   target="_blank">Show Diff</a>
                            </span>
                            {{/if}}
                        </div>
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
        </div>

        {{#each tables}}
            {{renderTablePreview .}}
        {{/each}}

        {{#if tags}}
            <a data-toggle="collapse" href="#collapseTags" class="collapsing-h2"><h2>Tags</h2></a>
            <div class="collapse" id="collapseTags">
            {{#each tags}}
                <span class="view-tag">{{this}}</span>
            {{/each}}
            </div>
        {{/if}}


{{/with}}